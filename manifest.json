{
	"name": "Quick Navigation",
	"version": "0.7",
	"description": "Navigate web using your keyboard without mouse. use ctrl+> to navigate and start typing",
	"content_scripts":[{
  		"matches": ["http://*/*","https://*/*"],
    	"js": ["jquery.js", "myscript.js"],
    	"css" : ["mystyle.css"],
    	"run_at" : "document_end"
	}],
	"permissions": [
  		"tabs",
		"http://*/*",
		"https://*/*"
  	],

  	"manifest_version": 2
}